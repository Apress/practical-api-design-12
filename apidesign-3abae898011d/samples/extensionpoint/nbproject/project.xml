<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1" xmlns:ns4="null">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>extensionpoint</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>extensionpoint</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>extensionpoint</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>src-hellodesign</label>
                    <type>java</type>
                    <location>src-hellodesign</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>src-helloworld</label>
                    <type>java</type>
                    <location>src-helloworld</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>build</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="run">
                    <target>run</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>build</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>build/api/classes</location>
                <build-target>build</build-target>
            </export>
            <export>
                <type>folder</type>
                <location>build/api2.0/classes</location>
                <build-target>build</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src-api</label>
                        <location>src-api</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src-helloworld</label>
                        <location>src-helloworld</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src-hellodesign</label>
                        <location>src-hellodesign</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="run"/>
                    <ide-action name="debug"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src-api</package-root>
                <classpath mode="compile">../libs/dist/org-openide-util-lookup.jar</classpath>
                <built-to>build/api/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>src-helloworld</package-root>
                <classpath mode="compile">src-api</classpath>
                <built-to>build/helloworld/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>src-hellodesign</package-root>
                <classpath mode="compile">src-api</classpath>
                <built-to>build/hellodesign/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
